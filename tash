[32m+[m[32m            function showPage(pageKey) {[m
[32m+[m[32m                Object.values(pageContents).forEach(content => content.classList.add('hidden'));[m
[32m+[m[32m                if (pageContents[pageKey]) {[m
[32m+[m[32m                    pageContents[pageKey].classList.remove('hidden');[m
[32m+[m[32m                }[m
[32m+[m[32m                navLinks.forEach(link => {[m
[32m+[m[32m                    link.classList.toggle('active', link.dataset.page === pageKey);[m
[32m+[m[32m                });[m
[32m+[m
[32m+[m[32m                if (pageKey === 'dashboard') {[m
[32m+[m[32m                    if (barChartInstance) barChartInstance.destroy();[m
[32m+[m[32m                    if (pieChartInstance) pieChartInstance.destroy();[m
[32m+[m[32m                    renderDashboardCharts();[m
[32m+[m[32m                } else if (pageKey === 'prediction') {[m
[32m+[m[32m                    // Reset prediction page to form view[m
[32m+[m[32m                    predictionResultsContent.classList.add('hidden');[m
[32m+[m[32m                    predictionFormContent.classList.remove('hidden');[m
[32m+[m[32m                    predictionError.classList.add('hidden');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // --- Event Listeners ---[m
[32m+[m[32m            loginButton.addEventListener('click', () => {[m
[32m+[m[32m                loginPage.classList.add('hidden');[m
[32m+[m[32m                appContainer.classList.remove('hidden');[m
[32m+[m[32m                appContainer.classList.add('flex');[m
[32m+[m[32m                showPage('dashboard');[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            logoutButton.addEventListener('click', () => {[m
[32m+[m[32m                appContainer.classList.add('hidden');[m
[32m+[m[32m                appContainer.classList.remove('flex');[m
[32m+[m[32m                loginPage.classList.remove('hidden');[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            navLinks.forEach(link => {[m
[32m+[m[32m                link.addEventListener('click', () => showPage(link.dataset.page));[m
[32m+[m[32m            });[m
[32m+[m[41m            [m
[32m+[m[32m            generatePredictionBtn.addEventListener('click', () => {[m
[32m+[m[32m                // 1. Validate form[m
[32m+[m[32m                const allFilled = Object.values(formInputs).every(input => input.value.trim() !== '');[m
[32m+[m[32m                if (!allFilled) {[m
[32m+[m[32m                    predictionError.classList.remove('hidden');[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m[32m                predictionError.classList.add('hidden');[m
[32m+[m
[32m+[m[32m                // 2. Show loading state[m
[32m+[m[32m                const btnText = generatePredictionBtn.querySelector('.btn-text');[m
[32m+[m[32m                btnText.textContent = 'Generating...';[m
[32m+[m[32m                generatePredictionBtn.disabled = true;[m
[32m+[m[32m                const spinner = document.createElement('i');[m
[32m+[m[32m                spinner.setAttribute('data-lucide', 'loader-2');[m
[32m+[m[32m                spinner.classList.add('w-5', 'h-5', 'animate-spin');[m
[32m+[m[32m                generatePredictionBtn.prepend(spinner);[m
[32m+[m[32m                lucide.createIcons({[m
[32m+[m[32m                    nodes: [spinner][m
[32m+[m[32m                });[m
[32m+[m
[32m+[m[32m                // 3. Simulate API call[m
[32m+[m[32m                setTimeout(() => {[m
[32m+[m[32m                    // Update result fields (can be dynamic later)[m
[32m+[m[32m                    document.getElementById('result-item-name').textContent = formInputs.itemName.value;[m
[32m+[m[32m                    document.getElementById('result-price').textContent = '$' + (formInputs.histPrice.value * 0.92).toFixed(2);[m
[32m+[m[32m                    document.getElementById('result-confidence').textContent = (Math.random() * (0.98 - 0.75) + 0.75).toFixed(2) * 100 + '% Confidence';[m
[32m+[m[32m                    document.getElementById('result-hist-price').textContent = '$' + formInputs.histPrice.value;[m
[32m+[m[32m                    document.getElementById('result-price-change').textContent = '-8.2%';[m
[32m+[m[32m                    document.getElementById('result-category').textContent = formInputs.category.value;[m
[32m+[m[32m                    document.getElementById('result-demand').textContent = formInputs.marketDemand.value + '/10';[m
[32m+[m[32m                    document.getElementById('result-season').textContent = document.getElementById('seasonal-factor').value;[m
[32m+[m
[32m+[m
[32m+[m[32m                    // 4. Switch views[m
[32m+[m[32m                    predictionFormContent.classList.add('hidden');[m
[32m+[m[32m                    predictionResultsContent.classList.remove('hidden');[m
[32m+[m[41m                    [m
[32m+[m[32m                    // 5. Reset button[m
[32m+[m[32m                    btnText.textContent = 'Generate Prediction';[m
[32m+[m[32m                    generatePredictionBtn.disabled = false;[m
[32m+[m[32m                    generatePredictionBtn.querySelector('i').remove();[m
[32m+[m
[32m+[m[32m                }, 2000);[m
[32m+[m[32m            });[m
[32m+[m
