[32m+[m[32mdocument.addEventListener('DOMContentLoaded', () => {[m
[32m+[m[32m    lucide.createIcons();[m
[32m+[m
[32m+[m[32m    // --- Page Routing / Logic ---[m
[32m+[m[32m    const pageId = document.body.querySelector('div[id$="-page"], div[id$="-container"]')?.id || '';[m
[32m+[m
[32m+[m[32m    // Login Page Logic[m
[32m+[m[32m    if (pageId.startsWith('login')) {[m
[32m+[m[32m        const loginButton = document.getElementById('login-button');[m
[32m+[m[32m        if (loginButton) {[m
[32m+[m[32m            loginButton.addEventListener('click', () => {[m
[32m+[m[32m                // In a real app, you'd perform authentication here[m
[32m+[m[32m                window.location.href = 'dashboard.html';[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Main App Logic (for all pages except login)[m
[32m+[m[32m    if (pageId.startsWith('app-container')) {[m
[32m+[m[32m        const logoutButton = document.getElementById('logout-button');[m
[32m+[m[32m        if (logoutButton) {[m
[32m+[m[32m            logoutButton.addEventListener('click', () => {[m
[32m+[m[32m                window.location.href = 'login.html';[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Dashboard Page Logic[m
[32m+[m[32m    if (document.getElementById('dashboard-page-content')) {[m
[32m+[m[32m        renderDashboardCharts();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Prediction Page Logic[m
[32m+[m[32m    if (document.getElementById('prediction-page-container')) {[m
[32m+[m[32m        setupPredictionPage();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Data Management Page Logic[m
[32m+[m[32m    if (document.getElementById('data-page-content')) {[m
[32m+[m[32m        setupDataManagementPage();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // --- Function Definitions ---[m
[32m+[m
[32m+[m[32m    function setupPredictionPage() {[m
[32m+[m[32m        const predictionFormContent = document.getElementById('prediction-form-content');[m
[32m+[m[32m        const predictionResultsContent = document.getElementById('prediction-results-content');[m
[32m+[m[32m        const generatePredictionBtn = document.getElementById('generate-prediction-btn');[m
[32m+[m[32m        const newPredictionBtn = document.getElementById('new-prediction-btn');[m
[32m+[m[32m        const predictionError = document.getElementById('prediction-error');[m
[32m+[m[32m        const formInputs = {[m
[32m+[m[32m            itemName: document.getElementById('item-name'),[m
[32m+[m[32m            category: document.getElementById('category'),[m
[32m+[m[32m            histPrice: document.getElementById('hist-price'),[m
[32m+[m[32m            marketDemand: document.getElementById('market-demand'),[m
[32m+[m[32m            seasonalFactor: document.getElementById('seasonal-factor')[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        generatePredictionBtn.addEventListener('click', () => {[m
[32m+[m[32m            const allFilled = Object.values(formInputs).every(input => input.value.trim() !== '');[m
[32m+[m[32m            if (!allFilled) {[m
[32m+[m[32m                predictionError.classList.remove('hidden');[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m            predictionError.classList.add('hidden');[m
[32m+[m
[32m+[m[32m            const btnText = generatePredictionBtn.querySelector('.btn-text');[m
[32m+[m[32m            btnText.textContent = 'Generating...';[m
[32m+[m[32m            generatePredictionBtn.disabled = true;[m
[32m+[m[32m            const spinner = document.createElement('i');[m
[32m+[m[32m            spinner.setAttribute('data-lucide', 'loader-2');[m
[32m+[m[32m            spinner.classList.add('w-5', 'h-5', 'animate-spin');[m
[32m+[m[32m            generatePredictionBtn.prepend(spinner);[m
[32m+[m[32m            lucide.createIcons({ nodes: [spinner] });[m
[32m+[m
[32m+[m[32m            setTimeout(() => {[m
[32m+[m[32m                const histPrice = parseFloat(formInputs.histPrice.value);[m
[32m+[m[32m                const predictedPrice = histPrice * 0.92;[m
[32m+[m[32m                const priceChange = ((predictedPrice - histPrice) / histPrice) * 100;[m
[32m+[m
[32m+[m[32m                document.getElementById('result-item-name').textContent = formInputs.itemName.value;[m
[32m+[m[32m                document.getElementById('result-price').textContent = '$' + predictedPrice.toFixed(2);[m
[32m+[m[32m                document.getElementById('result-confidence').textContent = (Math.random() * (98 - 75) + 75).toFixed(2) + '% Confidence';[m
[32m+[m[32m                document.getElementById('result-hist-price').textContent = '$' + histPrice.toFixed(2);[m
[32m+[m[41m                [m
[32m+[m[32m                const priceChangeEl = document.getElementById('result-price-change');[m
[32m+[m[32m                priceChangeEl.textContent = priceChange.toFixed(1) + '%';[m
[32m+[m[32m                priceChangeEl.classList.toggle('text-red-500', priceChange < 0);[m
[32m+[m[32m                priceChangeEl.classList.toggle('text-green-500', priceChange >= 0);[m
[32m+[m
[32m+[m[32m                document.getElementById('result-category').textContent = formInputs.category.value;[m
[32m+[m[32m                document.getElementById('result-demand').textContent = formInputs.marketDemand.value + '/10';[m
[32m+[m[32m                document.getElementById('result-season').textContent = formInputs.seasonalFactor.options[formInputs.seasonalFactor.selectedIndex].text;[m
[32m+[m[41m                [m
[32m+[m[32m                document.getElementById('market-analysis-text').textContent = `The ${formInputs.itemName.value} has seen stable global demand, but with a rating of ${formInputs.marketDemand.value}/10 in market demand, it indicates moderate interest. The seasonal factor suggests a mild impact on pricing. Historical pricing shows a solid base at $${histPrice.toFixed(2)}, but a predicted decline to $${predictedPrice.toFixed(2)} factors in current market saturation and lower new model demand.`;[m
[32m+[m
[32m+[m[32m                predictionFormContent.classList.add('hidden');[m
[32m+[m[32m                predictionResultsContent.classList.remove('hidden');[m
[32m+[m
[32m+[m[32m                btnText.textContent = 'Generate Prediction';[m
[32m+[m[32m                generatePredictionBtn.disabled = false;[m
[32m+[m[32m                generatePredictionBtn.querySelector('i').remove();[m
[32m+[m[32m            }, 2000);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        newPredictionBtn.addEventListener('click', () => {[m
[32m+[m[32m            predictionResultsContent.classList.add('hidden');[m
[32m+[m[32m            predictionFormContent.classList.remove('hidden');[m
[32m+[m[32m            predictionError.classList.add('hidden');[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function setupDataManagementPage() {[m
[32m+[m[32m        const fileUpload = document.getElementById('file-upload');[m
[32m+[m[32m        const fileUploadText = document.getElementById('file-upload-text');[m
[32m+[m[32m        if (fileUpload) {[m
[32m+[m[32m            fileUpload.addEventListener('change', () => {[m
[32m+[m[32m                if (fileUpload.files.length > 0) {[m
[32m+[m[32m                    fileUploadText.textContent = fileUpload.files[0].name;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    fileUploadText.textContent = 'Upload CSV or Excel File';[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function renderDashboardCharts() {[m
[32m+[m[32m        let barChartInstance, pieChartInstance;[m
[32m+[m
[32m+[m[32m        const barCtx = document.getElementById('barChart')?.getContext('2d');[m
[32m+[m[32m        if (barCtx) {[m
[32m+[m[32m            barChartInstance = new Chart(barCtx, {[m
[32m+[m[32m                type: 'bar',[m
[32m+[m[32m                data: {[m
[32m+[m[32m                    labels: ['Nike Air Jordan', 'MacBook Pro M3', 'iPhone 15 Pro'],[m
[32m+[m[32m                    datasets: [{[m
[32m+[m[32m                        label: 'Predicted Price',[m
[32m+[m[32m                        data: [220, 2550, 1100],[m
[32m+[m[32m                        backgroundColor: 'rgba(79, 70, 229, 0.8)',[m
[32m+[m[32m                        borderRadius: 8,[m
[32m+[m[32m                        barThickness: 30,[m
[32m+[m[32m                    }, {[m
[32m+[m[32m                        label: 'Market Average',[m
[32m+[m[32m                        data: [250, 2600, 1150],[m
[32m+[m[32m                        backgroundColor: 'rgba(203, 213, 225, 0.8)',[m
[32m+[m[32m                        borderRadius: 8,[m
[32m+[m[32m                        barThickness: 30,[m
[32m+[m[32m                    }][m
[32m+[m[32m                },[m
[32m+[m[32m                options: { responsive: true, maintainAspectRatio: false, scales: { y: { beginAtZero: true } } }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        const pieCtx = document.getElementById('pieChart')?.getContext('2d');[m
[32m+[m[32m        if (pieCtx) {[m
[32m+[m[32m            pieChartInstance = new Chart(pieCtx, {[m
[32m+[m[32m                type: 'pie',[m
[32m+[m[32m                data: {[m
[32m+[m[32m                    labels: ['Electronics', 'Clothing', 'Food', 'Automotive'],[m
[32m+[m[32m                    datasets: [{[m
[32m+[m[32m                        data: [50, 17, 17, 17],[m
[32m+[m[32m                        backgroundColor: ['#4f46e5', '#a78bfa', '#34d399', '#f59e0b'],[m
[32m+[m[32m                        borderColor: '#ffffff',[m
[32m+[m[32m                        borderWidth: 4,[m
[32m+[m[32m                    }][m
[32m+[m[32m                },[m
[32m+[m[32m                options: { responsive: true, maintainAspectRatio: false, plugins: { legend: { position: 'bottom' } } }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
